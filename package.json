{
  "name": "node-pixiv",
  "version": "0.1.0",
  "description": "A third-party Node.js library for Pixiv.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run compile && node chmod.js",
    "lint": "eslint src/ && eslint test/ -c .eslintrc.test.js",
    "compile": "babel src -d dist",
    "test": "npm run compile && mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HakurouKen/node-pixiv.git"
  },
  "keywords": [
    "pixiv",
    "api",
    "download",
    "illust"
  ],
  "bin": {
      "pixiv-download": "bin/download"
  },
  "author": "HakurouKen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HakurouKen/node-pixiv/issues"
  },
  "homepage": "https://github.com/HakurouKen/node-pixiv#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-es2017": "^6.14.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.7.0",
    "husky": "^0.11.9",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "bluebird": "^3.4.6",
    "cheerio": "^0.22.0",
    "commander": "^2.9.0",
    "extend": "^3.0.0",
    "lodash": "^4.16.4",
    "request": "^2.75.0"
  }
}
